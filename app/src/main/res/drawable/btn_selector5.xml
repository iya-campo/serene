<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
        <shape>
            <corners android:radius="30dp" />
            <solid android:color="#dce9eb" />
            <stroke android:width="5px" android:color="#7eb2c0" android:dashWidth="5px" android:dashGap="7px" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="30dp" />
            <solid android:color="#f2f8f9" />
            <stroke android:width="5px" android:color="#89b9c5" android:dashWidth="5px" android:dashGap="7px" />
        </shape>
    </item>

</selector>