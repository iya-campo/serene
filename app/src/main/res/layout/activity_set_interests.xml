<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  xmlns:tools="http://schemas.android.com/tools"
  tools:context=".SetInterests"
  android:background="@drawable/serenebg">

  <TextView
    android:id="@+id/interestsText"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="49dp"
    android:text="What are your interests?" />

  <Button
    android:id="@+id/next2Button"
    android:layout_width="40dp"
    android:layout_height="wrap_content"
    android:layout_alignBaseline="@+id/interestsText"
    android:layout_alignParentEnd="true"
    android:layout_marginEnd="64dp"
    android:text=">" />

  <ImageView
    android:id="@+id/animalsImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentStart="true"
    android:layout_alignParentTop="true"
    android:layout_marginStart="48dp"
    android:layout_marginTop="98dp"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/artImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignTop="@+id/animalsImg"
    android:layout_centerHorizontal="true"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/beautyImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentEnd="true"
    android:layout_alignTop="@+id/animalsImg"
    android:layout_marginEnd="48dp"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/entertainmentImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_alignStart="@+id/animalsImg"
    android:layout_marginTop="195dp"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/fashionImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignTop="@+id/entertainmentImg"
    android:layout_centerHorizontal="true"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/foodImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignStart="@+id/beautyImg"
    android:layout_alignTop="@+id/entertainmentImg"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/gamesImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:layout_alignStart="@+id/animalsImg"
    android:layout_marginBottom="143dp"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/healthImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignTop="@+id/gamesImg"
    android:layout_centerHorizontal="true"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/musicImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignStart="@+id/beautyImg"
    android:layout_alignTop="@+id/gamesImg"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/natureImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:layout_alignStart="@+id/animalsImg"
    android:layout_marginBottom="40dp"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/photographyImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignTop="@+id/natureImg"
    android:layout_centerHorizontal="true"
    app:srcCompat="@mipmap/ic_launcher_round" />

  <ImageView
    android:id="@+id/sportsImg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignStart="@+id/beautyImg"
    android:layout_alignTop="@+id/natureImg"
    app:srcCompat="@mipmap/ic_launcher_round" />

</RelativeLayout>
